{%  extends '::master_back.html.twig' %}
{% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
{% block body %}
    <div class="col-lg-5">
    {{ form_start(form) }}

    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading" style="text-align:center">
                Modifier Model Notification
            </div>

            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-hover">

                        <tr>
                                <td>Type</td>
                                <td>{{ form_widget(form.typeNotif) }}</td>
                        </tr>

                        <tr>
                            <td>Contenu</td>
                            <td>{{ form_widget(form.contenuNotif) }}</td>
                        </tr>

                        <tr>
                            <td></td>
                            <td>{{ form_widget(form.Valider) }}</td>
                        </tr>

                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
        </div>
    </div>

    {{ form_end(form) }}
    </div>
{%  endblock %}