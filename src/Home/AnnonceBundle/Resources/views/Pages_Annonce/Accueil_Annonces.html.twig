            console.log("{{  titre.titreAnnonce }}");
            availableTags.push('{{  titre.titreAnnonce }}');
//Array.prototype.push.apply(availableTags,' titre.titreAnnonce }}');
            {% endfor %}

            console.log(availableTags);
            jQuery( "#tags" ).autocomplete({
                source: availableTags
            });

        } );
    </script>
{% endblock %}